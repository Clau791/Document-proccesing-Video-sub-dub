{
  "name": "attack-hub-fullstack",
  "version": "1.0.0",
  "description": "Document processing for video and audio",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\" --names \"BACKEND,FRONTEND\" --prefix-colors \"bgBlue.bold,bgMagenta.bold\"",
    "backend": "cd backend && python -u app.py",
    "frontend": "cd frontend && npm run dev",
    "install-all": "npm run install-backend && npm run install-frontend",
    "install-backend": "cd backend && pip install -r requirements.txt",
    "install-frontend": "cd frontend && npm install",
    "build": "cd frontend && npm run build"
  },
  "keywords": [
    "security",
    "ddos",
    "testing"
  ],
  "author": "Clau",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.7.0",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "lucide-react": "^0.545.0",
    "motion": "^12.23.22"
  }
}
